SERVICE_NAME=kafbat-ui

# Host/container port mapping
HOST_KAFBAT_PORT=<KAFBAT_PORT>                   # Host port for Kafka UI
CONTAINER_KAFBAT_PORT=8080                       # Internal port

# Dynamic config: connect to your Kafka bootstrap
